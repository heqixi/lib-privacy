    <style name="BaseDialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="PrivacyAppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorAccent">@color/privacy_primary</item>
        <item name="android:colorControlHighlight">@android:color/transparent</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowBackground">@color/privacy_gray_f8</item>
        <item name="android:colorControlActivated">@color/privacy_primary</item>
    </style>
    <style name="PrivacyDialogStyle" parent="@style/BaseDialog">
        <item name="android:windowNoTitle">true</item>
        <!--dialog遮罩透明度-->
        <item name="android:backgroundDimAmount">0.6</item>
        <!--是否需要dialog遮罩-->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="PrivacySwitchStyle">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:thumb">@drawable/privacy_switch_thumb</item>
        <item name="android:background">@null</item>
        <item name="switchMinWidth">32dp</item>
    </style>
